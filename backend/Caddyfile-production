telemetry.0xt.ca {
    tls tanner@0xt.ca
    root /assets
    header / {
      -Server
      Cache-Control "no-cache, no-store, must-revalidate"
    }
    proxy /status http://vernemq:8888 {
      keepalive 32
      transparent
    }
    proxy /amqp http://rabbitmq:15672 {
      keepalive 32
      transparent
      without /amqp
    }
    proxy /api http://engine:6060 {
      keepalive 32
      transparent
    }
}
